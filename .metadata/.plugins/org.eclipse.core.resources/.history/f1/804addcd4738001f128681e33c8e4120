package threejava;

public class p1 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	 
     int lines =5;
     
     for(int j=1;j<lines;j++)
     {
    	 for (int i=1;i<=lines;i++)
    	 {
    		 System.out.print("i");
    		 
    	 }
           System.out.println();
         
     }
    
}
}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		