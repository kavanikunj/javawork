package threejava;

public class print_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         
		int rows = 4;
		
		for(int i = 1; i <= rows; i++) {
			
			for(int j = 1; j <= rows; j++) {
				if(j % 2 == 0) {
					System.out.println("i");
				} else {
					System.out.println("j");
				}
			}
			
			System.out.println();
		}	
	}

}
